{
  "name": "api-gateway",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn ./bin/www.ts",
    "test": "ts-node node_modules/jest/bin/jest.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "debug": "tsc-watch --onSuccess \"nodemon --inspect dist/bin/www.js\"",
    "start:docker": "docker start apigateway || bash -c 'docker run -d -e HOT_RELOAD=true -v /`pwd`/configs/apiConfig.yaml:/code/configs/apiConfig.yaml -p 30001:30001 --name apigateway apigateway'",
    "stop:docker": "docker stop apigateway"
  },
  "dependencies": {
    "express": "~4.16.1",
    "express-http-proxy": "^1.6.3",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^28.1.7",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.8",
    "@types/rewire": "^2.5.28",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "rewire": "^6.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
